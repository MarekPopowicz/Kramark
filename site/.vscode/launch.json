{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Listen for XDebbug",
            "type": "php",
            "request": "launch",
            "port": 9005
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9005
        },
        {
            "type": "node-terminal",
            "name": "Create JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}/site.js"
        }
    ]
}